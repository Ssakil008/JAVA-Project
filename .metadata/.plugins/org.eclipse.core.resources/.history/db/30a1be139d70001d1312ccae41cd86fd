
public class array {

	public static void main(String[] args) {
		// array = used to store multiple values in a single varibles
		
		String[] cars = new String[3];
		
		cars[0] = "Camaro";
		cars[1] = "corvette";
		cars[2] = "Tesla";
		
		for(int i=0; i<cars.length; i++) {
			System.out.println(cars[i]);
		}

	}

}
